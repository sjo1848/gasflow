{
  "name": "gasflow-mobile",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "expo": "^54.0.0",
    "react": "19.1.0",
    "react-native": "0.81.0"
  },
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@types/react": "^19.1.17",
    "babel-jest": "29.7.0",
    "babel-preset-expo": "^54.0.10",
    "jest": "29.7.0",
    "react-test-renderer": "19.1.0",
    "typescript": "5.6.3"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/__tests__"
